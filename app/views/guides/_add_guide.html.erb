<div class="guide">
  <p>
    <b>Guide name:</b> <%= link_to guide.first_name + " " + guide.last_name, guide_path(guide, from: @start_date, to: @end_date) %>
  </p>
  <p><b>Email:</b> <%= guide.email %></p>
  <p><b>Car Available:</b> <%= guide.has_car %></p>
  <p>
    <b> <img src="<%= guide.photo %>"> </b>
  </p>

  <%= link_to 'Book', guide_add_guide_path(guide), method: :post, id: "add_guide" %>
</div>