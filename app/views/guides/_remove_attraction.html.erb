<div class="attraction">
  <img src="<%= attraction.image_url %>" alt="edit" />
  <a href="<%=attraction.url%>" target="_blank"><%= attraction.name %></a>
  <%= link_to 'Add', yelps_add_path(session[:traveler_id], attraction: attraction.name, location: @location), method: :post, id: 'add_attraction', class: "add-remove-but" %>
</div>
